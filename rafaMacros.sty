\newcommand{\degree}{\ensuremath{ ^\circ }}

\newcommand{\vect}  [1]{\ensuremath{ \mathsf{vec}  \left( #1 \right) }}
\newcommand{\Diag}  [1]{\ensuremath{ \mathsf{Diag} \left( #1 \right) }}
\newcommand{\diag}  [1]{\ensuremath{ \mathsf{diag} \left( #1 \right) }}
\newcommand{\rank}  [1]{\ensuremath{ \mathsf{rank} \left( #1 \right) }}
\newcommand{\spark} [1]{\ensuremath{ \mathsf{spark}\left( #1 \right) }}
\newcommand{\trace} [1]{\ensuremath{ \mathsf{Tr}   \left( #1 \right) }}
\newcommand{\card}  [1]{\ensuremath{ \mathsf{card} \left( #1 \right) }}
\newcommand{\supp}  [1]{\ensuremath{ \mathsf{supp} \left( #1 \right) }}

\newcommand{\prob}  [1]{\ensuremath{ \mathsf{Pr}   \left\{ #1 \right\} }}
\newcommand{\expval}[1]{\ensuremath{ \mathsf{E}    \left\{ #1 \right\} }}
\newcommand{\var}   [1]{\ensuremath{ \mathsf{Var}  \left\{ #1 \right\} }}
\newcommand{\cov}   [1]{\ensuremath{ \mathsf{Cov}  \left\{ #1 \right\} }}

\newcommand{\norm}  [2]{\ensuremath{ \left \lVert #1 \right \rVert_{#2} }}
\newcommand{\hypo}  [2]{\ensuremath{ \overset{ #2 }{\underset{ #1 }{\gtreqless}} }}

\newcommand{\zerobf}{\mathbf{0}}
\newcommand{\onebf} {\mathbf{1}}

\DeclareMathOperator*{\minimize}{\textrm{minimize}}
\DeclareMathOperator*{\maximize}{\textrm{maximize}}

\DeclareMathOperator*{\argmin}{\textrm{argmin}}
\DeclareMathOperator*{\argmax}{\textrm{argmax}}

\newcounter{optimization}[chapter]
\newenvironment{optimization}{\refstepcounter{optimization}\equation}{\tag{P-\thechapter.\theoptimization}\endequation}

\theoremstyle{definition}
\newtheorem{definition}{Definition}[chapter]

\theoremstyle{remark}
\newtheorem{remark}{Remark}[chapter]

